{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      showDropdown: false,\n      pages: 1,\n      //ÊâÄÊúâÈ°µÈù¢\n      current: 1,\n      //ÂΩìÂâçÈ°µÈù¢\n      userarr: [],\n      currentPageBook: [],\n      //ÂΩìÂâçÈ°µÁöÑÊâÄÊúâÂõæ‰π¶\n      displayarr: [{\n        value: \"2\"\n      }],\n      item_num: 10,\n      // ÊâÄÊúâÂõæ‰π¶ÁöÑÊÄªÊï∞\n      page_num: 6,\n      index: 0,\n      previewImage: '',\n      showdig: false,\n      URL: \"\",\n      bookimgurl: \"\",\n      formData: {\n        b_Id: \"\",\n        b_Name: \"\",\n        b_Author: \"\",\n        b_Press: \"\",\n        b_Pub_Year: \"\",\n        b_Total_Collection: \"\",\n        b_Cur_Collection: \"\",\n        b_Bs_Id: \"\",\n        b_Isbn: \"\",\n        b_Price: \"\",\n        b_Description: \"\",\n        image: null\n      },\n      searchForm: {\n        bookTitle: \"\",\n        author: \"\",\n        bookNumber: \"\",\n        publisher: \"\",\n        ISBN: \"\"\n      },\n      favDialogVisible: false\n    };\n  },\n  methods: {\n    logout() {\n      // Âú®ËøôÈáåÊâßË°åÈÄÄÂá∫ÁôªÂΩïÁöÑÊìç‰Ωú\n      this.$router.replace('/admLogin');\n    },\n    handleFileChange(event) {\n      const file = event.target.files[0]; // Ëé∑ÂèñÁî®Êà∑ÈÄâÊã©ÁöÑÊñá‰ª∂\n      this.image = file; // Êõ¥Êñ∞ÁªÑ‰ª∂‰∏≠ÁöÑimageÂèòÈáè\n\n      // ‰ΩøÁî® FileReader Êù•ÂÆûÁé∞È¢ÑËßàÂõæÁâá\n      const reader = new FileReader();\n      reader.onload = e => {\n        this.previewImage = e.target.result;\n      };\n      reader.readAsDataURL(file);\n    },\n    updata_arr() {\n      var left = (this.current - 1) * this.page_num;\n      var right = left + this.page_num;\n      this.displayarr = []; // ÂÖàÊ∏ÖÁ©∫Êï∞ÁªÑ\n      for (; left < right && left < this.userarr.length; left++) {\n        this.displayarr.push(this.userarr[left]); // Â∞ÜÊñ∞Êï∞ÊçÆËµãÂÄºÁªôÊï∞ÁªÑ\n      }\n    },\n\n    leftmove() {\n      if (this.current > 1) {\n        this.current = this.current - 1;\n        this.searchBooksFunc(this.URL, this.current);\n      }\n    },\n    rightmove() {\n      if (this.current < this.pages) {\n        this.current++;\n        this.searchBooksFunc(this.URL, this.current);\n      }\n    },\n    chooseFile() {\n      // ÂàõÂª∫<input>ÂÖÉÁ¥†\n      var fileInput = document.createElement('input');\n      fileInput.type = 'file';\n\n      // Ê∑ªÂä†change‰∫ã‰ª∂ÁõëÂê¨Âô®ÔºåÂΩìÈÄâÊã©Êñá‰ª∂ÂêéÊâßË°åÂõûË∞ÉÂáΩÊï∞\n      fileInput.addEventListener('click', () => {\n        const input = document.createElement('input');\n        input.type = 'file';\n        input.accept = 'image/*'; // ÂèØ‰ª•Ê†πÊçÆÈúÄË¶Å‰øÆÊîπÊñá‰ª∂Á±ªÂûãÈôêÂà∂\n        input.onchange = event => {\n          const selectedFile = event.target.files[0];\n          if (selectedFile) {\n            // Âú®ËøôÈáåÂèØ‰ª•ÊâßË°åÊñá‰ª∂‰∏ä‰º†ÁöÑÈÄªËæë\n            console.log('Â∑≤ÈÄâÊã©Êñá‰ª∂:', selectedFile.name);\n          }\n        };\n        input.click();\n      });\n\n      // Ëß¶ÂèëÊñá‰ª∂ÈÄâÊã©ÂØπËØùÊ°Ü\n      fileInput.click();\n    },\n    toArr(temparr) {\n      //Â∞Ü‰π¶Á±çÂ≠òÂÇ®Âà∞userarrÊï∞ÁªÑ\n      this.userarr = temparr;\n      this.pages = Math.ceil(this.userarr.length / this.page_num);\n      this.current = 1;\n      this.updata_arr();\n    },\n    openDialog() {\n      const favDialog = document.getElementById(\"favDialog\");\n      if (typeof favDialog.showModal === \"function\") {\n        favDialog.showModal();\n        console.log(\"hello?\");\n      } else {\n        this.outputBox = \"Sorry, the <dialog> API is not supported by this browser.\";\n      }\n    },\n    openDialog2(tempindex) {\n      this.index = tempindex;\n      this.showdig = true;\n      this.bookimgurl = \"https://web-bmsystem.oss-cn-hangzhou.aliyuncs.com/books/\" + this.currentPageBook[this.index].b_Id + \".png\";\n      const favDialog = document.getElementById(\"favDialog2\");\n      if (typeof favDialog.showModal === \"function\") {\n        favDialog.showModal();\n        console.log(\"hello?\");\n      } else {\n        this.outputBox = \"Sorry, the <dialog> API is not supported by this browser.\";\n      }\n    },\n    cancelForm() {\n      const favDialog = document.getElementById(\"favDialog\");\n      favDialog.reset(); // Reset form fields\n      favDialog.close(); // Close the dialog\n    },\n\n    cancelForm2() {\n      this.showdig = false;\n      const favDialog = document.getElementById(\"favDialog\");\n      favDialog.reset(); // Reset form fields\n      favDialog.close(); // Close the dialog\n    },\n\n    submitForm() {\n      const config = {\n        headers: {\n          \"Content-Type\": \"application/json\"\n        }\n      };\n      this.formData.b_Price = parseFloat(this.formData.b_Price);\n      this.formData.b_Total_Collection = parseInt(this.formData.b_Total_Collection);\n      this.formData.b_Cur_Collection = parseInt(this.formData.b_Cur_Collection);\n      this.formData.b_Pub_Year = parseInt(this.formData.b_Pub_Year);\n      // ÂèëÈÄÅ POST ËØ∑Ê±Ç\n      axios.post(\"/api/add_book\", this.formData, config).then(response => {\n        // this.$refs.form.reset();\n        this.favDialogVisible = false;\n        console.log(\"Ë°®ÂçïÊï∞ÊçÆÂ∑≤ÊàêÂäü‰º†ËæìÂà∞ÂêéÁ´Ø\");\n        console.log(response.data);\n        console.log(response);\n      }).catch(error => {\n        console.error(\"ËØ∑Ê±ÇÂ§±Ë¥•:\", error);\n      });\n    },\n    submitForm2() {\n      const config = {\n        headers: {\n          \"Content-Type\": \"application/json\"\n        }\n      };\n      this.currentPageBook[this.index].b_Price = parseFloat(this.currentPageBook[this.index].b_Price);\n      this.currentPageBook[this.index].b_Total_Collection = parseInt(this.currentPageBook[this.index].b_Total_Collection);\n      this.currentPageBook[this.index].b_Cur_Collection = parseInt(this.currentPageBook[this.index].b_Cur_Collection);\n      this.currentPageBook[this.index].b_Pub_Year = parseInt(this.currentPageBook[this.index].b_Pub_Year);\n      // ÂèëÈÄÅ POST ËØ∑Ê±Ç\n      axios.patch(\"/api/modify_book\", this.currentPageBook[this.index], config).then(response => {\n        // this.$refs.form.reset();\n        this.favDialogVisible = false;\n        console.log(\"Ë°®ÂçïÊï∞ÊçÆÂ∑≤ÊàêÂäü‰º†ËæìÂà∞ÂêéÁ´Ø\");\n        console.log(response.data);\n        console.log(response);\n      }).catch(error => {\n        console.error(\"ËØ∑Ê±ÇÂ§±Ë¥•:\", error);\n      });\n    },\n    deleteBook(tempindex) {\n      this.index = tempindex;\n      const bookIdToDelete = this.currentPageBook[this.index].b_Id;\n      axios.delete(`/api/delete_book/${bookIdToDelete}`).then(response => {\n        console.log(\"Ë°®ÂçïÊï∞ÊçÆÂ∑≤ÊàêÂäü‰º†ËæìÂà∞ÂêéÁ´Ø\");\n        console.log(response.data);\n        console.log(response);\n        this.searchBooks();\n        alert(\"‰π¶Á±çÂà†Èô§ÊàêÂäüÔºÅ\");\n      }).catch(error => {\n        console.error(\"ËØ∑Ê±ÇÂ§±Ë¥•:\", error);\n      });\n      this.searchBooks();\n    },\n    searchBooks() {\n      this.current = 1;\n      this.URL = \"/api/books?key=\";\n      if (this.searchForm.bookTitle != \"\") {\n        this.URL = this.URL + \"name&value=\" + this.searchForm.bookTitle;\n      } else if (this.searchForm.ISBN != \"\") {\n        this.URL = this.URL + \"ISBN&value=\" + this.searchForm.ISBN;\n      } else if (this.searchForm.author != \"\") {\n        this.URL = this.URL + \"author&value=\" + this.searchForm.author;\n      } else if (this.searchForm.bookNumber != \"\") {\n        this.URL = this.URL + \"id&value=\" + this.searchForm.bookNumber;\n      } else if (this.searchForm.publisher != \"\") {\n        this.URL = this.URL + \"press&value=\" + this.searchForm.publisher;\n      }\n      this.searchBooksFunc(this.URL, this.current); //ÂèëÈÄÅËØ∑Ê±Ç\n    },\n\n    searchBooksFunc(Url, page) {\n      const that = this; // ‰øùÂ≠òÁªÑ‰ª∂ÂÆû‰æãÁöÑÂºïÁî®\n\n      const nextPageURL = Url + \"&page=\" + page + \"&pageSize=6\";\n      axios.get(nextPageURL).then(response => {\n        const jsdata = response.data;\n        // Â§ÑÁêÜ‰ø°ÊÅØ\n        that.currentPageBook = jsdata.data.bookList;\n        that.current = page;\n        that.pages = Math.ceil(jsdata.data.amount / 6);\n      }).catch(error => {\n        console.error(\"ËØ∑Ê±ÇÂ§±Ë¥•:\", error);\n      });\n    }\n  },\n  mounted() {\n    // Âä†ËΩΩÊó∂ËØ∑Ê±ÇÊâÄÊúâ‰π¶Á±ç\n  }\n};","map":{"version":3,"names":["axios","data","showDropdown","pages","current","userarr","currentPageBook","displayarr","value","item_num","page_num","index","previewImage","showdig","URL","bookimgurl","formData","b_Id","b_Name","b_Author","b_Press","b_Pub_Year","b_Total_Collection","b_Cur_Collection","b_Bs_Id","b_Isbn","b_Price","b_Description","image","searchForm","bookTitle","author","bookNumber","publisher","ISBN","favDialogVisible","methods","logout","$router","replace","handleFileChange","event","file","target","files","reader","FileReader","onload","e","result","readAsDataURL","updata_arr","left","right","length","push","leftmove","searchBooksFunc","rightmove","chooseFile","fileInput","document","createElement","type","addEventListener","input","accept","onchange","selectedFile","console","log","name","click","toArr","temparr","Math","ceil","openDialog","favDialog","getElementById","showModal","outputBox","openDialog2","tempindex","cancelForm","reset","close","cancelForm2","submitForm","config","headers","parseFloat","parseInt","post","then","response","catch","error","submitForm2","patch","deleteBook","bookIdToDelete","delete","searchBooks","alert","Url","page","that","nextPageURL","get","jsdata","bookList","amount","mounted"],"sources":["src/views/BookManage.vue"],"sourcesContent":["<template>\n    <div class=\"app\">\n        <dialog id=\"favDialog\" style=\"position: absolute;z-index: 10; width: 40%;\" >\n            <form method=\"dialog\" id=\"myform\">\n            <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Id\" class=\"item1\">‰π¶Á±çÁºñÂè∑:</label>\n                <input v-model=\"formData.b_Id\" type=\"text\" id=\"b_Id\" name=\"b_Id\" class=\"item1\">\n            </div>\n            <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Name\" class=\"item1\" >‰π¶Á±çÂêçÁß∞:</label>\n                <input v-model=\"formData.b_Name\" type=\"text\" id=\"b_Name\" name=\"b_Name\"  class=\"item1\">\n            </div>\n            <div class=\"upload-container\" style=\"position: absolute;right: 25px;top:40px\">\n                <div class=\"upload-icon\">\n                    üìÅ\n                </div>\n                <div class=\"upload-label\">\n                    <img v-bind:src=\"previewImage\" style=\"max-width: 100px;\" alt=\"‰∏ä‰º†ÂõæÁâá\">\n                </div>\n                <label for=\"fileInput\" class=\"custom-button\">ÈÄâÊã©Êñá‰ª∂</label>\n                <input @change=\"handleFileChange\" type=\"file\" id=\"fileInput\" class=\"file-input\">\n            </div>\n            \n            <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Author\" class=\"item1\">‰ΩúËÄÖ:</label>\n                <input v-model=\"formData.b_Author\" type=\"text\" id=\"b_Author\" name=\"b_Author\" class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Press\" class=\"item1\">Âá∫ÁâàÁ§æ:</label>\n                <input v-model=\"formData.b_Press\" type=\"text\" id=\"b_Press\" name=\"b_Press\"  class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Pub_Year\" class=\"item1\">Âá∫ÁâàÂπ¥‰ªΩ:</label>\n                <input v-model=\"formData.b_Pub_Year\" type=\"text\" id=\"b_Pub_Year\" name=\"b_Pub_Year\" class=\"item1\">\n            </div> \n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Total_Collection\"  class=\"item1\">ÊÄªËóè‰π¶Èáè:</label>\n                <input v-model=\"formData.b_Total_Collection\" type=\"text\" id=\"b_Total_Collection\" name=\"b_Total_Collection\" class=\"item1\" >\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Cur_Collection\"  class=\"item1\">ÂΩìÂâçËóè‰π¶Èáè:</label>\n                <input v-model=\"formData.b_Cur_Collection\" type=\"text\" id=\"b_Cur_Collection\" name=\"b_Cur_Collection\"  class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:230px;\">\n                <label for=\"b_Bs_Id\"  class=\"item1\">‰π¶Á±çÂàÜÁ±ªÁºñÂè∑:</label>\n                <input v-model=\"formData.b_Bs_Id\" type=\"text\" id=\"b_Bs_Id\" name=\"b_Bs_Id\"   class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Isbn\"  class=\"item1\">ISBN:</label>\n                <input v-model=\"formData.b_Isbn\" type=\"text\" id=\"b_Isbn\" name=\"b_Isbn\"  class=\"item1\" >\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Price\"  class=\"item\">‰ª∑Ê†º:</label>\n                <input v-model=\"formData.b_Price\" type=\"text\" id=\"b_Price\" name=\"b_Price\"  class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Description\"  class=\"item1\">ÊèèËø∞:</label>\n                <input v-model=\"formData.b_Description\" type=\"text\" id=\"b_Description\" name=\"b_Description\"  class=\"item1\">\n            </div>\n                <!--button @click=\"chooseFile\">‰∏ä‰º†ÂõæÁâá</button-->\n                \n                <input type=\"submit\" value=\"Êèê‰∫§\" @click=\"submitForm\">\n                <button id=\"cancelbutton\" @onclick=\"cancelForm\">ÂèñÊ∂à</button>\n              </form>\n          </dialog>\n          \n          <dialog id=\"favDialog2\" style=\"position: absolute;z-index: 10; width: 40%;\" >\n            <form v-if=\"currentPageBook.length>0&&showdig\" method=\"dialog\" id=\"myform2\">\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Id\" class=\"item1\">‰π¶Á±çÁºñÂè∑:</label>\n                <input v-model=\"currentPageBook[index].b_Id\" type=\"text\" id=\"b_Id2\" name=\"b_Id2\" class=\"item1\">\n            </div>\n            <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Name\" class=\"item1\" >‰π¶Á±çÂêçÁß∞:</label>\n                <input v-model=\"currentPageBook[index].b_Name\" type=\"text\" id=\"b_Name\" name=\"b_Name\"  class=\"item1\">\n            </div>\n            <div class=\"upload-container\" style=\"position: absolute;right: 25px;top:40px\">\n                <div class=\"upload-label\">\n                    <img v-bind:src=bookimgurl style=\"max-width: 100px;\" alt=\"‰∏ä‰º†ÂõæÁâá\">\n                </div>\n            </div>\n\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Author\" class=\"item1\">‰ΩúËÄÖ:</label>\n                <input v-model=\"currentPageBook[index].b_Author\" type=\"text\" id=\"b_Author\" name=\"b_Author\" class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Press\" class=\"item1\">Âá∫ÁâàÁ§æ:</label>\n                <input v-model=\"currentPageBook[index].b_Press\" type=\"text\" id=\"b_Press\" name=\"b_Press\"  class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Pub_Year\" class=\"item1\">Âá∫ÁâàÂπ¥‰ªΩ:</label>\n                <input v-model=\"currentPageBook[index].b_Pub_Year\" type=\"text\" id=\"b_Pub_Year\" name=\"b_Pub_Year\" class=\"item1\">\n            </div> \n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Total_Collection\"  class=\"item1\">ÊÄªËóè‰π¶Èáè:</label>\n                <input v-model=\"currentPageBook[index].b_Total_Collection\" type=\"text\" id=\"b_Total_Collection\" name=\"b_Total_Collection\" class=\"item1\" >\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Cur_Collection\"  class=\"item1\">ÂΩìÂâçËóè‰π¶Èáè:</label>\n                <input v-model=\"currentPageBook[index].b_Cur_Collection\" type=\"text\" id=\"b_Cur_Collection\" name=\"b_Cur_Collection\"  class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:230px;\">\n                <label for=\"b_Bs_Id\"  class=\"item1\">‰π¶Á±çÂàÜÁ±ªÁºñÂè∑:</label>\n                <input v-model=\"currentPageBook[index].b_Bs_Id\" type=\"text\" id=\"b_Bs_Id\" name=\"b_Bs_Id\"   class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Isbn\"  class=\"item1\">ISBN:</label>\n                <input v-model=\"currentPageBook[index].b_Isbn\" type=\"text\" id=\"b_Isbn\" name=\"b_Isbn\"  class=\"item1\" >\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Price\"  class=\"item\">‰ª∑Ê†º:</label>\n                <input v-model=\"currentPageBook[index].b_Price\" type=\"text\" id=\"b_Price\" name=\"b_Price\"  class=\"item1\">\n            </div>\n                <div style=\"display: inline-block; width:200px;\">\n                <label for=\"b_Description\"  class=\"item1\">ÊèèËø∞:</label>\n                <input v-model=\"currentPageBook[index].b_Description\" type=\"text\" id=\"b_Description\" name=\"b_Description\"  class=\"item1\">\n            </div>\n\n                <input type=\"submit\" value=\"‰øÆÊîπ\" @click=\"submitForm2\">\n                <button id=\"cancelbutton\" @onclick=\"cancelForm2\">ÂèñÊ∂à</button>\n              </form>\n          </dialog>\n\n        <header class=\"headLine\">\n            <img alt=\"\" src=\"../../resource/image/logo0.png\" style=\"height: 80px;position: absolute;top: 10px;left: 15%;\">\n            <img alt=\"\" src=\"../../resource/image/line.png\" style=\"height: 80px;position: absolute;top: 10px;left: 35%;\">\n            <router-link to=\"/AdmHome\" >\n                <p  class=\"textstyle\" >È¶ñÈ°µ</p>\n            </router-link>\n            \n            <div @mouseenter=\"showDropdown=true\" @mouseleave=\"showDropdown = false\">\n            <span class=\"textstyle\" style=\"left:80%;z-index: 7;\"  >ÁÆ°ÁêÜÂëòÔºåÊÇ®Â•ΩÔºÅ</span>\n            <div class=\"dropdown\" v-if=\"showDropdown\" style=\"z-index: 6;\"  @mouseenter=\"showDropdown=true\" >\n              <ul  @mouseenter=\"showDropdown=true\">\n                <li @click=\"logout\" >ÈÄÄÂá∫ÁôªÂΩï</li>\n              </ul>\n            </div>\n          </div>\n        </header>\n        <div class=\"contain\">\n        \n        <div class=\"box\">\n            <div class=\"leftbox \">\n                <div>\n                    <p>ÊúçÂä°‰ø°ÊÅØ</p>\n                    <p>Âõæ‰π¶Êü•ËØ¢</p>\n                    <div class=\"card\">\n                    <p id=\"new_book\" @click=\"openDialog\">Êñ∞Â¢ûÂõæ‰π¶</p>\n                    </div>\n                </div>\n            </div>\n            \n\n            <div class=\"container\">\n                <div class=\"contain\">\n                    <form >\n                        <div style=\"display: inline-block;\">\n                           <label for=\"bookTitle\" class=\"item\">‰π¶ÂêçÔºö</label>\n                         <input type=\"text\" id=\"bookTitle\" v-model=\"searchForm.bookTitle\" placeholder=\"ËØ∑ËæìÂÖ•‰π¶Âêç\" class=\"item\" style=\"width: 60%;\"> \n                        </div>\n                    <div style=\"display: inline-block;\">\n                        <label for=\"author\" class=\"item\" >‰ΩúËÄÖÔºö</label>\n                        <input type=\"text\" v-model=\"searchForm.author\" id=\"author\" placeholder=\"ËØ∑ËæìÂÖ•‰ΩúËÄÖ\" class=\"item \" style=\"width: 60%;\" >\n                    </div>\n                    <div style=\"display: inline-block;\">\n                  <label for=\"bookNumber\" class=\"item\">Âõæ‰π¶ÁºñÂè∑Ôºö</label>\n                  <input type=\"text\" id=\"bookNumber\" v-model=\"searchForm.bookNumber\" placeholder=\"ËØ∑ËæìÂÖ•Âõæ‰π¶ÁºñÂè∑\" class=\"item \" style=\"width: 50%;\">\n                  </div>\n                  <div style=\"display: inline-block;\">\n                  <label for=\"publisher\" class=\"item\">Âá∫ÁâàÁ§æÔºö</label>\n                  <input type=\"text\" id=\"publisher\" v-model=\"searchForm.publisher\" placeholder=\"ËØ∑ËæìÂÖ•Âá∫ÁâàÁ§æ\" style=\"width: 50%;\">\n                </div>\n                <div style=\"display: inline-block;\" class=\"itembox\">\n                  <label for=\"category\" class=\"item\">ISBNÔºö</label>\n                  <input type=\"text\" id=\"category\" v-model=\"searchForm.ISBN\" placeholder=\"ËØ∑ËæìÂÖ•Á±ªÂà´\" class=\"item\" style=\"width: 60%;\">\n                </div>\n                  <button type=\"submit\" style=\"position: absolute; right: 20%\" @click=\"searchBooks\">Êü•ËØ¢</button>\n                </form>\n                </div>\n                \n              <div class=\"move\">\n                <table id=\"resultsTable\">\n                  <tr>\n                    <th>‰π¶Âêç</th>\n                    <th>‰ΩúËÄÖ</th>\n                    <th>Âõæ‰π¶ÁºñÂè∑</th>\n                    <th>Âá∫ÁâàÁ§æ</th>\n                    <th>ISBN</th>\n                    <th></th>\n                    <th></th>\n                  </tr>\n                  <!-- Êü•ËØ¢ÁªìÊûúÂ∞ÜÂú®ËøôÈáåÂä®ÊÄÅÊ∑ªÂä† -->\n                  <tr v-for=\"(item,index) in currentPageBook\" :key=\"index\">\n                    <th>{{item.b_Name}}</th>\n                    <th>{{item.b_Author}}</th>\n                    <th>{{item.b_Id}}</t