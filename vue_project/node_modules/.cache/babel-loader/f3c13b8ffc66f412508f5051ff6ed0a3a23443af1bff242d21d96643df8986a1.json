option(pagename);\n      }\n    },\n    // 手动改变当前页码时执行的操作\n    choose_page: function choose_pae() {\n      var cur_page = document.getElementById('currentPage').value;\n      var pagename = page_index[parseInt(cur_page) - 1];\n      page_option(pagename);\n    },\n    // 鼠标事件，经过每一条资讯时改变标题的颜色\n    light: function (obj) {\n      obj.firstElementChild.style.color = \"#FF9900\";\n    },\n    normal: function (obj) {\n      obj.firstElementChild.style.color = \"#000000\";\n    }\n  },\n  mounted() {\n    //发送异步请求，获取数据\n    axios.get(\"\");\n  }\n};","map":{"version":3,"names":["axios","MessageDate","data","input","page_index","methods","page_option","pagename","tar_index","indexOf","slice","j","length","close_div","document","getElementsByClassName","i","style","display","opendiv","first_click","getElementById","value","last_click","total_page","prev_click","cur_page","parseInt","next_click","choose_page","choose_pae","light","obj","firstElementChild","color","normal","mounted","get"],"sources":["src/views/MessageView.vue"],"sourcesContent":["<template>\r\n    <body>  \r\n      <!--左侧导航栏-->\r\n      <div class=\"sidenav\">\r\n        <div class=\"LogoBox\">\r\n        <img src=\"../../resource/logo.png\" style=\"height: 35px;\">\r\n        </div>\r\n        <div class=\"TextBox\">\r\n        <div>Library management system</div>\r\n        </div>\r\n        <div class='card-holder' style=\"margin-top: 70px;\">\r\n          <div class='card-wrapper'>\r\n          <router-link to='/message'>\r\n          <div class='card bg-01'>\r\n          <span class='card-content'>图书基本信息 #1</span>\r\n          </div>\r\n          </router-link>\r\n          </div>\r\n          <div class='card-wrapper'>\r\n          <router-link to=\"/return\">\r\n          <div class='card bg-02'>\r\n          <span class='card-content'>归还图书 #2</span>\r\n          </div>\r\n          </router-link>\r\n          </div>\r\n          <div class='card-wrapper'>\r\n          <router-link to='/home'>\r\n    