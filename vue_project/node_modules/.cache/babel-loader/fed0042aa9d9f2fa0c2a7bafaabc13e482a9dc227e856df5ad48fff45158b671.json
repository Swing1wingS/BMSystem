olute;\r\n            backface-visibility:hidden;\r\n            perspective: 1000px;\r\n            transform: rotateY(-180deg);\r\n            transition: 0.5s ease-in-out;\r\n            display:flex;\r\n            flex-direction: column;\r\n\r\n        }\r\n        .maskText{\r\n          width:280px;\r\n          height:300px;\r\n          font-family: '微软雅黑';\r\n          flex-direction: row;\r\n          align-content: center;\r\n          \r\n        }\r\n        .box1:hover .box{\r\n            transform: rotateY(-180deg);\r\n        }\r\n        .box1:hover .mask{\r\n            transform: rotateY(-360deg);\r\n        }\r\n\r\n.BoxRow{\r\n  width:80%;\r\n  margin-right: 80px;\r\n  height: 400px;\r\n  align-items: center;\r\n  display: flex;\r\n  justify-content: space-around;\r\n}\r\n  </style>"],"mappings":"AAwHA,OAAAA,KAAA;AACA,OAAAC,WAAA;AACA;EAEAC,KAAA;IACA;MACAD,WAAA;MACAE,WAAA;MACAC,KAAA;MACAC,OAAA;MAAA;MACAC,WAAA;MAAA;MACAC,WAAA;QACAC,GAAA;QACAC,KAAA;QACAC,IAAA;QACAC,QAAA;MACA;IAGA;EAGA;EACA,MAAAC,QAAA;IACAZ,KAAA,CAAAa,GAAA;MACAC,MAAA;QACAN,GAAA,OAAAD,WAAA,CAAAC,GAAA;QACAC,KAAA,OAAAF,WAAA,CAAAE,KAAA;QACAC,IAAA,OAAAH,WAAA,CAAAG,IAAA;QACAC,QAAA,OAAAJ,WAAA,CAAAI;MACA;IACA,GAAAI,IAAA,CAAAC,MAAA;MACAC,OAAA,CAAAC,GAAA,CAAAF,MAAA;MACA,KAAAb,WAAA,GAAAa,MAAA,CAAAd,IAAA,CAAAA,IAAA,CAAAiB,QAAA;MACA,KAAAd,OAAA,GAAAW,MAAA,CAAAd,IAAA,CAAAA,IAAA,CAAAkB,MAAA;IACA;EAEA;EACAC,OAAA;IACAC,QAAA;MACAtB,KAAA,CAAAa,GAAA;QACAC,MAAA;UACAN,GAAA,OAAAD,WAAA,CAAAC,GAAA;UACAC,KAAA,OAAAF,WAAA,CAAAE,KAAA;UACAC,IAAA,OAAAH,WAAA,CAAAG,IAAA;UACAC,QAAA,OAAAJ,WAAA,CAAAI;QACA;MACA,GAAAI,IAAA,CAAAC,MAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,MAAA;QACA,KAAAb,WAAA,GAAAa,MAAA,CAAAd,IAAA,CAAAA,IAAA,CAAAiB,QAAA;QACA,KAAAd,OAAA,GAAAW,MAAA,CAAAd,IAAA,CAAAA,IAAA,CAAAkB,MAAA;MACA;IACA;IACAG,oBAAAC,GAAA;MACAC,KAAA;MACA,KAAAlB,WAAA,CAAAG,IAAA,GAAAc,GAAA;MACA,KAAAF,OAAA;IACA;EAEA;EACAI,QAAA;IACA;EAAA;AAGA"},"metadata":{},"sourceType":"module","externalDependencies":[]}